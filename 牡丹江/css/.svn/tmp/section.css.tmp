/*郑存杰*/
.job{padding:19px 26px 10px 30px;}
.zcj_mainWrap{width:100%;padding:0 15px 15px;box-sizing:border-box;}
/*内容头部-------------------------*/
.zcj_mainTop{padding-left:24px;border-bottom:1px solid #e5e5e5;background:#fff;border-radius:7px 7px 0 0;height:51px;}
.zcj_mainTopL{font-size:1.4rem;color:#222;margin-right:38px;}
.zcj_mainTopL span{line-height:52px;}
/*主题内容------------------------*/
.zcj_main{border-radius:0 0 7px 7px;background:#fff;padding:0 20px 0 28px;box-sizing:border-box;}
.zcj_main .main_mainLeft{width:71%;}
.zcj_main .main_mainLeft .mainContent{color:#5093e1;font-size:1.4rem;}
.zcj_main .main_mainLeft .tit{line-height:60px;font-weight:600;}
.zcj_main .main_mainLeft .txt{
	max-height: 200px;
	overflow-y: auto;
	color:#222;
	line-height:22px;
	padding:23px 32px 28px 20px;
	border:1px solid #ccc;
	background:#f5f5f5;
}
.zcj_main .main_mainLeft .txt table td,th{border:1px solid #ddd;}
.zcj_main .main_mainLeft .fujian{padding:8px 0 14px 3px;}
.zcj_main .main_mainLeft .pingjia{font-size:1.4rem;}
.zcj_main .main_mainLeft .pingjia .tit{color:#5093e1;}
.zcj_main .main_mainLeft .pingjia textarea{outline:none;min-height:122px;display:block;width:100%;box-sizing:border-box;}
.zcj_main .main_mainLeft .subWrap{height:62px;padding-top:20px;}
.zcj_main .main_mainLeft .subWrap .btn{background:#cce0f6;width:96px;height:33px;line-height:33px;color:#5093e1;font-size:1.4rem;text-align:center;border-radius:16px;cursor:pointer;}
.zcj_main .main_mainLeft .subWrap .btn.shangchuan{background: #F5F5F5;color: #000000;}
/*回复开始----------------------------------*/
.commMicroMusicRWrap{float:right;width:24%;margin: 23px 1.5% 0 3%;}
.messageWrap{height:50px;width:100%;overflow:auto;line-height:50px;font-size:1.4rem;color:#333;}
.messagePerson{padding-left:14%;float:right;}
.messageNum{float:right;padding-left:7%;background:url(../img/eye.png) no-repeat left center;margin-left:6%;color:#5093e1;}
.messageNum2{background-image:url(../img/xiaz.png);background-position:left 16px;background-size:16px;color:#50c14e;}
.messageBox{border:1px solid #eee;border-radius:2px;position:relative;padding-bottom:46px;}
.messageBox ul{max-height:400px;overflow:auto;min-height:100px;}
.messageBox li{line-height:28px;font-size:1.4rem;color:#333;padding:0 8px;}
.messageBox li .huifu{padding:0 2px 0 5px;}
.messageBox li .mesageName{color:#5093e1;white-space:nowrap;word-wrap:break-word;cursor:pointer;}
.messageBox li .content{word-break:break-all;word-wrap:break-word;}
.moreHf{font-size:1.4rem;color:#5093e1;line-height:28px;padding-right:8px;text-align:right;padding-bottom:17px;}
/*留言内容------------*/
#messageForm{padding:0 8px;}
.messageContent{box-sizing:border-box;width:100%;height:100px;overflow:auto;outline:none;border:0 none;background:#f5f5f5;line-height:20px;font-size:1.4rem;color:#333;padding:7px 9px 0 9px;}
#messageForm input{position:absolute;right:8px;bottom:8px;width:68px;height:30px;background:#5093e1;color:#fff;font-size:1.4rem;text-align:center;line-height:30px;border-radius:5px;border:none;outline:none;}

.commMusic>p{margin: 10px 0; text-align: center; color: #5093e1; cursor: pointer;}
.commMusic>p i{display: inline-block; font-style: normal; width: 16px; height: 12px; background: url(../img/xiangxia.png) no-repeat center;}
